{
  "ConnectionStrings": {
    "Default": "__DB_CONNECTION_STRING__"
  },
  "RabbitMQ": {
    "HostName": "__RABBITMQ_HOST__",
    "Port": 5671,
    "UserName": "__RABBITMQ_USER__",
    "Password": "__RABBITMQ_PASSWORD__",
    "QueueName": "imu-data-queue",
    "PrefetchCount": 50,
    "MaxConcurrency": 20
  },
  "Redis": {
    "Endpoint": "__REDIS_ENDPOINT__",
    "UseSsl": true,
    "Database": 0,
    "KeyPrefix": "imu:session:",
    "ExpirationHours": 4
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "IPSDataAcquisitionWorker": "Information"
    }
  }
}

